generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id          Int      @id @unique @default(autoincrement())
  login       String   @unique
  displayName String
  email       String
  dAuth       Boolean  @default(false)
  avatar      String
  createdAt   DateTime @default(now())
  updatedAt   DateTime
  lastLogin   DateTime @default(now())
}
