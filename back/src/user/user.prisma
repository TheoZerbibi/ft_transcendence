model User {
	id			Int	@id @default(autoincrement()) @unique
	login		String @unique
	displayName	String
	email		String
	dAuth		Boolean @default(false)
	avatar		String
	createdAt	DateTime @default(now())
	updatedAt	DateTime @updatedAt
	lastLogin	DateTime @default(now())
	GamePlayer 	GamePlayer[]

	@@map("users")
}
