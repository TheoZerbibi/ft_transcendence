model User {
	id				Int	@id @default(autoincrement())
	login			String @unique
	display_name	String
	email			String
	dAuth			Boolean @default(false)
	avatar			String
	created_at		DateTime @default(now())
	updated_at		DateTime @updatedAt
	last_login		DateTime @default(now())
	channelUser		ChannelUser[]

	@@map("users")
}
